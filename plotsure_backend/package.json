{
  "name": "plotsure-connect-backend",
  "version": "1.0.0",
  "description": "Backend API for PlotSure Connect - Land verification platform using MySQL",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "migrate": "npx sequelize-cli db:migrate",
