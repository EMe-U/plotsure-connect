  },
  "keywords": [
    "real-estate",
    "land-verification",
    "rwanda",
    "bugesera",
    "api",
    "mysql"
  ],
  "author": "Emerance Umurerwa",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.4-lts.1",
    "mysql2": "^3.6.5",
    "nodemailer": "^6.9.8",
    "sequelize": "^6.35.0",
    "sequelize-cli": "^6.6.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=14.0.0"
